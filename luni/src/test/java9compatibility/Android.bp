// Android tests related to compatibility with pre-Java 11 language features.

java_library {
    name: "core-java-9-compatibility-tests",
    hostdex: true,
    srcs: [
        "java/**/*.java",
    ],
    java_version: "1.9",
    sdk_version: "none",
    system_modules: "core-all-system-modules",
    static_libs: [
        "core-test-rules",
        "junit",
        "junit-params",
    ],
    visibility: ["//libcore"],
}
